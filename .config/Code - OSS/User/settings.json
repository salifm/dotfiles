    "go.useLanguageServer": true,
    "go.buildOnSave": "off",
    "go.gotoSymbol.ignoreFolders": [
        ".ignore"
    ],
    "go.toolsGopath": "/home2/salif/go",
    "go.lintOnSave": "off",
    "terminal.integrated.rendererType": "dom",
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "c",
        "cpp",
